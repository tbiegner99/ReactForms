{
  "name": "@tbiegner99/react-forms",
  "version": "1.1.6",
  "description": "A component utility library for react",
  "main": "./build/esm/src/main.mjs",
  "type": "module",
  "sideEffects": false,
  "directories": {
    "test": "tests"
  },
  "exports": {
    "import": "./build/esm/src/main.mjs",
    "require": "./build/cjs/src/main.js"
  },
  "scripts": {
    "test": "jest --config jest.config.cjs",
    "coverage": "npm run test -- --coverage",
    "test-watch": "jest --config jest.config.cjs --watch",
    "test-debug": "node --inspect ./node_modules/jest/bin/jest.js --config jest.config.cjs --watch ",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "storybook-setup": "getstorybook",
    "build-storybook": "build-storybook",
    "lint-fix": "eslint --fix src tests",
    "lint": "eslint src tests",
    "prepush": "npm run lint && npm run coverage",
    "prepublishOnly": "rm -rf build && npm run build",
    "build": "rollup -c ./rollup.config.js",
    "watch": "rollup -c ./rollup.config.js -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tbiegner99/ReactForms"
  },
  "keywords": [
    "forms",
    "react",
    "validation"
  ],
  "author": "TJ Biegner",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.6.0",
    "radium": "^0.26.1"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "radium": "^0.26.1",
    "react": "^16.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/addons": "^6.4.13",
    "@storybook/cli": "^6.4.13",
    "@storybook/react": "^6.4.13",
    "@testing-library/jest-dom": "^5.11.9",
    "babel-jest": "^27.4.6",
    "babel-plugin-module-resolver": "^3.0.0",
    "css-loader": "^6.5.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^0.14.3",
    "jest": "^27.4.7",
    "jsdoc": "^3.6.6",
    "prettier": "^2.5.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rollup": "^2.38.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "storybook": "^6.4.13",
    "style-loader": "^3.3.1",
    "webpack": "^5.66.0"
  }
}
